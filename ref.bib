@article{Morales_2021,
	doi = {10.3847/1538-4357/ac1104},
	url = {https://doi.org/10.3847/1538-4357/ac1104},
	year = 2021,
	month = {oct},
	publisher = {American Astronomical Society},
	volume = {919},
	number = {2},
	pages = {120},
	author = {Alexa M. Morales and Charlotte A. Mason and Sean Bruton and Max Gronke and Francesco Haardt and Claudia Scarlata},
	title = {The Evolution of the Lyman-alpha Luminosity Function during Reionization},
	journal = apj,
	abstract = {The time frame in which hydrogen reionization occurred is highly uncertain, but can be constrained by observations of Lyman-alpha (LyŒ±) emission from distant sources. Neutral hydrogen in the intergalactic medium (IGM) attenuates LyŒ± photons emitted by galaxies. As reionization progressed the IGM opacity decreased, increasing LyŒ± visibility. The galaxy LyŒ± luminosity function (LF) is thus a useful tool to constrain the timeline of reionization. In this work, we model the LyŒ± LF as a function of redshift, z = 5‚Äì10, and average IGM neutral hydrogen fraction, . We combine the LyŒ± luminosity probability distribution obtained from inhomogeneous reionization simulations with a model for the UV LF to model the LyŒ± LF. As the neutral fraction increases, the average number density of LyŒ± emitting galaxies decreases, and are less luminous, though for there is only a small decrease in the LyŒ± LF. We use our model to infer the IGM neutral fraction at z = 6.6, 7.0, and 7.3 from observed LyŒ± LFs. We conclude that there is a significant increase in the neutral fraction with increasing redshift: and . We predict trends in the LyŒ± luminosity density and Schechter parameters as a function of redshift and the neutral fraction. We find that the LyŒ± luminosity density decreases as the universe becomes more neutral. Furthermore, as the neutral fraction increases, the faint-end slope of the LyŒ± LF steepens, and the characteristic LyŒ± luminosity shifts to lower values; hence, we conclude that the evolving shape of the LyŒ± LF‚Äînot just its integral‚Äîis an important tool to study reionization.}
}

